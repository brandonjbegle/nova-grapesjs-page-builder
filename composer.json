{
    "name": "brandonjbegle/nova-grapesjs-page-builder",
    "description": "Laravel Nova GrapesJS Page Builder Tool",
    "keywords": [
        "laravel",
        "nova",
        "grapesjs"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "BrandonJBegle\\NovaGrapesjsPageBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BrandonJBegle\\NovaGrapesjsPageBuilder\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
